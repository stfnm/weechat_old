msgid "[-current] [-exclude=<channel>[,<channel>...]] <command> [<arguments>]"
msgstr "[-current] [-exclude=<channel>[,<channel>...]] <command> [<arguments>]"

msgid ""
" -current: execute command for channels of current server only\n"
