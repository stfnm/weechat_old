"  /connect freenode\n"
"  /connect irc.oftc.net/6667\n"
"  /connect irc6.oftc.net/6667 -ipv6\n"
"  /connect irc6.oftc.net/6697 -ipv6 -ssl\n"
"  /connect my.server.org/6697 -ssl -password=test\n"
